{
    "version": 2,
    "buildCommand": "pnpm build",
    "devCommand": "pnpm dev",
    "installCommand": "pnpm install",
    "framework": null,
    "outputDirectory": "dist",
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api/index"
      }
    ],
    "functions": {
      "api/**/*.ts": {
        "runtime": "nodejs20.x"
      }
    },
    "env": {
      "NODE_ENV": "production"
    }
  }